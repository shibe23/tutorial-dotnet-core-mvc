@model IEnumerable<tutorial_dotnet_core_mvc.Models.Book>

<table class="table">
  <thead>
    <tr>
      <th>書名</th>
      <th>価格</th>
      <th>出版社</th>
      <th>配布サンプル</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var item in Model)
    {
      <tr>
        <td>@item.Title</td>
        <td>@item.Price</td>
        <td>@item.Publisher</td>
        <td>@item.Sample</td>
      </tr>
    }
  </tbody>
</table>